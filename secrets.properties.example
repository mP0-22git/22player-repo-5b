# Secrets Configuration for Trebl Music Player
# =============================================
# Copy this file to secrets.properties and fill in your values
# DO NOT commit secrets.properties to version control!

# Google Play License Key
# Get this from: Play Console > Your App > Monetization setup > Licensing
GOOGLE_PLAY_LICENSE_KEY=your_license_key_here

# AdMob IDs
# Get these from: AdMob Console > Apps > Your App
# Use test IDs during development: https://developers.google.com/admob/android/test-ads
ADMOB_APP_ID=ca-app-pub-3940256099942544~3347511713
ADMOB_BANNER_ID=ca-app-pub-3940256099942544/6300978111

# Release Signing Config
# These are only needed for release builds
STORE_FILE=/path/to/your/keystore.jks
STORE_PASSWORD=your_store_password
KEY_ALIAS=your_key_alias
KEY_PASSWORD=your_key_password
